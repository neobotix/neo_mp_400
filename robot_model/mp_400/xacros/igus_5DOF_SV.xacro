            <mu2>0.2</mu2>
        </gazebo>

        <!-- Link4 -->
        <gazebo reference="link4">
            <material>Gazebo/White</material>
            <kp>1000000</kp>
            <kd>1000000</kd>
            <mu1>0.2</mu1>
            <mu2>0.2</mu2>
        </gazebo>

        <!-- Link5 -->
        <gazebo reference="link5">
            <material>Gazebo/White</material>
            <kp>1000000</kp>
            <kd>1000000</kd>
            <mu1>0.2</mu1>
            <mu2>0.2</mu2>
        </gazebo>
    </xacro:macro>
 
</robot>

