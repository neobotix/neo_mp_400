<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="igus_5DOF_SV" params="parent prefix *origin">
    <joint name="linear_joint" type="fixed">
      <parent link="${parent}" />
      <child link = "linear_link" />
      <origin xyz="0.0 0.0 0.375" rpy="-1.57 0.0 1.57" />
      <axis xyz="0 0 1"/>
      <limit effort="30" velocity="1.0" />
    </joint>

    <joint name="arm_joint" type="prismatic">
      <parent link="linear_link" />
      <child link = "igus_base_link" />
      <origin xyz="0.0 -0.148 0.0" rpy="3.14 0 1.57" />
      <axis xyz="-1 0 0"/>
      <limit effort="100" velocity="1.0" lower = "0.0" upper = "1.0"/>
      <dynamics damping="1000.0"/>
    </joint>

    <link name="linear_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/vertical_axis.dae"  scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/vertical_axis.dae"  scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
     <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="30.0"/>
      <inertia ixx="0.05" ixy="0.05" ixz="0.05" iyy="0.05" iyz="0.05" izz="0.05"/>
    </inertial>
  </link>
   
  <link name="igus_base_link">
    <visual>
	  <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint0.dae"  scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint0Coll.dae" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.1045" ixy="-0.000460884801" ixz="0.000101130383"
                    iyy="0.105" iyz="0.002941" izz="0.01284"/>
    </inertial>
  </link>
  <link name="link1">
    <visual>
    <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint1.dae"  scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint1Coll.dae" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
  </link>
  <link name="link2">
    <visual>
    <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint2.dae"  scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint2Coll.dae" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
  </link>
  <link name="link3">
    <visual>
    <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint3.dae"  scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint3Coll.dae" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
  </link>
  <link name="link4">
    <visual>
    <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint4.dae"  scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint4Coll.dae" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
  </link>
  <link name="link5">
    <visual>
    <origin rpy="0 0 0" xyz="0 0 -0.0045"/>
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint5.dae"  scale="0.001 0.001 0.001"/>
        </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 -0.0045" />
      <geometry>
          <mesh filename="package://neo_mp_400/robot_model/mp_400/meshes/Joint5Coll.dae" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>
  </link>
  <joint name="joint1" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="igus_base_link"/>
    <child link="link1"/>
    <origin rpy="0 0 0" xyz="0 0 0.1275"/>
    <limit effort="100" velocity="30"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
 <joint name="joint2" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link1"/>
    <child link="link2"/>
    <origin rpy="0 0 0" xyz="0 0 0.10025"/>
    <limit effort="100" velocity="30"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
 <joint name="joint3" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link2"/>
    <child link="link3"/>
    <origin rpy="0 1.57079632679 0" xyz="0 0 0.270"/>
    <limit effort="100" velocity="30"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
 <joint name="joint4" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link3"/>
    <child link="link4"/>
    <origin rpy="0 0 0" xyz="0 0 0.240"/>
    <limit effort="100" velocity="30"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
 <joint name="joint5" type="continuous">
    <axis xyz="0 0 1"/>
    <parent link="link4"/>
    <child link="link5"/>
    <origin rpy="0 0 0" xyz="0 0 0.1835"/>
    <limit effort="100" velocity="30"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>
  <transmission name="tran_prismatic">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="arm_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="tran_prismatic_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran_roll">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint1">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="tran_roll_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran_1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint2">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="tran_1_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

   <transmission name="tran_2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint3">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="tran_2_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

   <transmission name="tran_3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint4">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="tran_3_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

   <transmission name="tran_4">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="joint5">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
    <actuator name="tran_4_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

    <gazebo reference="linear_link">
            <material>Gazebo/Grey</material>
            <kp>1000000</kp>
            <kd>1000000</kd>
            <mu1>500</mu1>
            <mu2>200</mu2>
        </gazebo>

    <gazebo reference="igus_base_link">
            <material>Gazebo/Grey</material>
            <kp>1000000</kp>
            <kd>1000000</kd>
            <mu1>1</mu1>
            <mu2>1</mu2>
        </gazebo>

        <!-- Link1 -->
        <gazebo reference="link1">
            <material>Gazebo/White</material>
            <kp>1000000</kp>
            <kd>1000000</kd>
            <mu1>0.2</mu1>
            <mu2>0.2</mu2>
        </gazebo>

        <!-- Link2 -->
        <gazebo reference="link2">
            <material>Gazebo/White</material>
            <kp>1000000</kp>
            <kd>1000000</kd>
            <mu1>0.2</mu1>
            <mu2>0.2</mu2>
        </gazebo>

        <!-- Link3 -->
        <gazebo reference="link3">
            <material>Gazebo/White</material>
           <kp>1000000</kp>
            <kd>1000000</kd>
