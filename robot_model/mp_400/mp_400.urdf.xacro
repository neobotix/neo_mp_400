<link name="base_link" />

<!-- Base link macro -->
 <xacro:mp_400_base name="base_footprint" parent="base_link">
    <origin xyz="0 0 0" rpy="0 0 0" />
  </xacro:mp_400_base>

  <!-- Wheel Macro -->
  <xacro:caster_wheel name="mp_400_wheel_front_left" parent="base_footprint">
    <origin xyz="0.164 0.158 0.036" rpy="0 0 1.57" />
  </xacro:caster_wheel>

  <xacro:caster_wheel name="mp_400_wheel_back_left" parent="base_footprint">
    <origin xyz="-0.319 0.158 0.036" rpy="0 0 1.57" />
  </xacro:caster_wheel>

  <xacro:caster_wheel name="mp_400_wheel_front_right" parent="base_footprint">
    <origin xyz="0.164 -0.158 0.036" rpy="0 0 1.57" />
  </xacro:caster_wheel>

   <xacro:caster_wheel name="mp_400_wheel_back_right" parent="base_footprint">
    <origin xyz="-0.319 -0.158 0.036" rpy="0 0 1.57" />
  </xacro:caster_wheel>

  <xacro:wheel_right name="mp_400_wheel_right" parent="base_footprint">
    <origin xyz="0. -0.26 0.074" rpy="0 0 1.57" />
  </xacro:wheel_right>

  <xacro:wheel_left name="mp_400_wheel_left" parent="base_footprint">
    <origin xyz="-0. 0.26 0.074" rpy="0 0 1.57" />
  </xacro:wheel_left>


  <!--+++++++++++++++++++laserscanner_link++++++++++++++++++++++++-->
<!-- S300, NanoScan and Microscan -->
  <xacro:sick_laser_v0 name="lidar_1" parent="base_link" ros_topic="lidar_1/scan_filtered">
    <origin xyz="0.230 0 0.110" rpy="0 3.14 3.14" />
  </xacro:sick_laser_v0>

<!-- Igus -->
<xacro:igus_5DOF_SV parent="base_link" prefix="igus">
    <origin xyz="0 0 0." rpy="0 0 3.14" />
  </xacro:igus_5DOF_SV> 

